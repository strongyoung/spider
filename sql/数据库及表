create database forconsumer;

create table seed(auto_id int not null auto_increment,link varchar(256),own varchar(50),is_visited tinyint ,is_valid tinyint,primary key(auto_id));

create table product(link varchar(512),product_id varchar(32),own varchar(20),title varchar(200),is_visited tinyint(1),is_valid tinyint(1),primary key(link));